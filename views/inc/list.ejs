<div id="content" class="app-content">

    <div class="container">

        <div class="row justify-content-center">

            <div class="col-xl-12">

                <div class="row">

                    <div class="col-xl-9">
                        <h1 class="page-header">
                          LIISKA MACAAMIISHA 
                          <small>
                            <% if (caaqil == 'active') { %>
                              Qeybta aan lacagtooda qaadanin
                            <% } else if (caaqil == 'expired') { %>
                              Qeybta lacagtooda qaateen
                            <% } %>
                          </small>
                          </h1>
                        <hr class="mb-4" />

                        <div id="datatable" class="mb-5">
                            <div class="card">
                                <div class="card-body">
                                    <table id="datatableDefault" class="table text-nowrap w-100">
                                        <thead>
                                            <tr>
                                                <th>#</th>
                                                <th>Magaca</th>
                                                <th>Number ka</th>
                                                <th>Deegaanka</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <%   var c = 1 ;   data.forEach(element => { %>
                                            <tr>
                                                <td> <%= c %> </td>
                                                <td> <%= element.magaca %>  </td>
                                                <td> <%= element.phone %></td>
                                                <td> <%= element.dagan %></td>
                                                <td><a href="/view/<%= element.link %> " class="btn btn-outline-warning btn-xs"><i class="fas fa-eye"></i></a></td>
                                            </tr>
                                            <% c++  }) %>
                                          
                                        </tbody>
                                    </table>
                                </div>
                                <div class="card-arrow">
                                    <div class="card-arrow-top-left"></div>
                                    <div class="card-arrow-top-right"></div>
                                    <div class="card-arrow-bottom-left"></div>
                                    <div class="card-arrow-bottom-right"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>